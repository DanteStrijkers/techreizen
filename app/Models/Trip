<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;

class Trip extends Model
{
    use HasFactory;

    protected $table = 'reizen'; // Tabelnaam als het niet de standaard is (meervoud van de modelnaam)

    protected $fillable = [
        'naam',
        'contact_email',
    ];
}
